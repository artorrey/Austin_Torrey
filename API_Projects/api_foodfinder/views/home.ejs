
<header><% include ../partials/header.ejs %></header>

<div class="container">
    <div class="row">
        <div class="col-sm-2"></div>
        <div class="col-sm-6">
            <h1>Find a food place near you!</h1>
        </div>
    </div>
    <div class="row">
        <div class="col-sm-2"></div>
        <form action="/searchresults" method="GET" class="col-sm-6" id="weatherlookup">
